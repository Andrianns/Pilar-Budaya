{
  "version": 2,
  "builds":[
    {
      "src":"/api/app.js",
      "use":"@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/app.js"
    }
  ],
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }],
  "installCommand": "npm install -g pg",
  "cleanUrls": true,
  "trailingSlash": false
}
